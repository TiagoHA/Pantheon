{
  "name": "panteon-server",
  "version": "0.1.0",
  "author": "LFSCamargo",
  "dependencies": {
    "apollo-server": "2.2.2",
    "bcryptjs": "2.4.3",
    "graphql": "0.13.2",
    "graphql-tag": "2.10.0",
    "jsonwebtoken": "8.4.0",
    "mongoose": "5.3.13",
    "ts-node": "7.0.1",
    "typescript": "3.1.6"
  },
  "devDependencies": {
    "@types/bcryptjs": "2.4.2",
    "@types/graphql": "14.0.3",
    "@types/jsonwebtoken": "8.3.0",
    "@types/mongoose": "5.3.1",
    "nodemon": "1.18.6",
    "tslint": "5.11.0"
  },
  "private": true,
  "main": "src/server.ts",
  "scripts": {
    "docker:up": "docker-compose up --build",
    "start": "ts-node",
    "watch": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec 'ts-node' src/server.ts"
  }
}
